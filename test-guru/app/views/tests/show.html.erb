<%= turbo_page_requires_reload %>
  <p>
    <strong>Title:</strong>
    <%= @test.title %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= @test.level %>
  </p>

  <p>
    <strong>Questions:</strong>
    <% @test.questions.each do |question| %>
      <ul>
        <li>
          <%= question.body %>
        </li>
      </ul>
    <% end %>
  </p>


<%= link_to 'All Tests', tests_path %> |
<%= link_to 'Edit', edit_test_path(@test) %>
