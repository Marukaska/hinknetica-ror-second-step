<%= turbo_frame_tag 'new_test_form' do %>
  <h1><%= question_header(@test, @question) %></h1>
  <%= form_with model: [@test, @question] do |form| %>
    <%= render 'shared/errors', resource: @question%>

    <p>
      <%= form.label :body %>
      <%= form.text_field :body %>
    </p>

    <p><%= form.submit %></p>
  <% end %>
<% end %>